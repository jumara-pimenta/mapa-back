SHADOW_DATABASE_URL=sqlserver://ENDEREÇO-DO-BANCO-DE-DADOS:PORTA-DO-BANCO-DE-DADOS;database=NOME-DO-BANCO-DE-DADOS;user=USUARIO-DO-BANCO-DE-DADOS;password=SENHA-DO-BANCO-DE-DADOS;encrypt=DANGER_PLAINTEXT

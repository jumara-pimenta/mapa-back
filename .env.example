# Variaveis banco de dados mongodb

ENVIRONMENT=development
BACKEND_PORT=3001

#Dados de conexão do Banco de dados rotas
#DATABASE_MSSQL_PASSWORD=SENHA-DO-BANCO-DE-DADOS
#DATABASE_MSSQL_USERNAME=USUARIO-DO-BANCO-DE-DADOS
